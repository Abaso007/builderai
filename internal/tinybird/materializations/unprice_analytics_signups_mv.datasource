SCHEMA >
    `date` Date,
    `session_id` String,
    `customer_id` String,
    `plan_version_id` String,
    `page_id` String,
    `status` LowCardinality(String),
    `unique_signups` AggregateFunction(uniq, String)

ENGINE AggregatingMergeTree
ENGINE_PARTITION_KEY toYYYYMM(date)
ENGINE_SORTING_KEY date, customer_id, page_id, plan_version_id
