TOKEN "web-apps" READ

NODE get_current_domain
SQL >

    WITH (
    SELECT nullif(domainWithoutWWW(url),'') as domain
    FROM unprice_page_events
    WHERE timestamp >= now() - interval 1 hour
    GROUP BY domain
    ORDER BY count(1) DESC
    LIMIT 1
    ) AS top_domain,
    (
    SELECT domainWithoutWWW(url)
    FROM unprice_page_events
    WHERE url NOT LIKE '%localhost%'
    LIMIT 1
    ) AS some_domain
    SELECT coalesce(top_domain, some_domain) AS domain

TYPE endpoint