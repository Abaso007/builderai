SCHEMA >
    `date` Date,
    `session_id` String,
    `plan_version_id` String,
    `plan_slug` LowCardinality(String),
    `plan_version` String,
    `page_id` String,
    `unique_clicks` AggregateFunction(uniq, String),
    `total_clicks` AggregateFunction(count)

ENGINE AggregatingMergeTree
ENGINE_PARTITION_KEY toYYYYMM(date)
ENGINE_SORTING_KEY date, session_id, page_id, plan_version_id
