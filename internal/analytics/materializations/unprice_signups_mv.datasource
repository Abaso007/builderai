SCHEMA >
    `date` Date,
    `plan_version_id` String,
    `customer_id` String,
    `page_id` String,
    `final_status` AggregateFunction(argMax, String, Int64),
    `total_signup_events` AggregateFunction(count)

ENGINE AggregatingMergeTree
ENGINE_PARTITION_KEY toYYYYMM(date)
ENGINE_SORTING_KEY date, plan_version_id, customer_id, page_id
ENGINE_TTL date + INTERVAL 6 MONTH