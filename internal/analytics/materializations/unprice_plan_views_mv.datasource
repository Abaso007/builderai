SCHEMA >
    `date` Date,
    `session_id` String,
    `plan_version_id` String,
    `page_id` String,
    `unique_views` AggregateFunction(uniq, String),
    `total_views` AggregateFunction(count)

ENGINE AggregatingMergeTree
ENGINE_PARTITION_KEY toYYYYMM(date)
ENGINE_SORTING_KEY date, page_id, plan_version_id, session_id
ENGINE_TTL date + INTERVAL 6 MONTH