DESCRIPTION >
    Get a heatmap of usage of features per plan version

TOKEN "web-apps" READ

NODE get_heatmap_node
SQL >
    %
    SELECT
        *
    FROM unprice_plan_versions
    JOIN unprice_plan_version_features
        ON unprice_plan_versions.id = unprice_plan_version_features.plan_version_id
    JOIN unprice_features
        ON unprice_plan_version_features.feature_id = unprice_features.id

TYPE endpoint
