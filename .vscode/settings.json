    "apikey",
    "apikeys",
    "asociated",
    "builderai",
    "Configurator",
    "datasource",
    "dinero",
    "hget",
    "hgetall",
    "hset",
    "ianvs",
    "ingestions",
    "LEMONSQUEEZY",
    "openai",
    "openapi",
    "satoshi",
    "sidemodule",
    "tinybird",
    "typecheck",
    "uidx",
    "Unprice",
    "UPSTASH"
  ],
  "search.exclude": {
    "**/.git": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/**/.next": true,
    "**/tmp": true
  }
}
